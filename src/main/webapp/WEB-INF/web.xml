<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Message</servlet-name>
  	<display-name>Message</display-name>
  	<description></description>
  	<servlet-class>com.controller.Message</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
  	<description></description>
  	<servlet-class>com.controller.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteMessage</servlet-name>
  	<display-name>DeleteMessage</display-name>
  	<description></description>
  	<servlet-class>com.controller.DeleteMessage</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Message</servlet-name>
  	<url-pattern>/Message</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteMessage</servlet-name>
  	<url-pattern>/DeleteMessage</url-pattern>
  </servlet-mapping>
</web-app>
